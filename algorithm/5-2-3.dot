graph BST {
    node [shape=circle];

    n8  [label="8"];
    n11 [label="11"];
    n4  [label="4"];
    n2  [label="2"];
    n3  [label="3"];
    n9  [label="9"];
    n6  [label="6"];
    n10 [label="10"];

    // 親子関係（無向）
    n8 -- n4;
    n8 -- n11;

    n4 -- n2;
    n4 -- n6;

    n2 -- n3;

    n11 -- n9;
    n9  -- n10;
}
