graph AVL_Delete {
    node [shape=circle];

    /* ノード定義: IDとラベルを分離してエラーを回避 */
    n5  [label="5"];
    n2  [label="2"];
    n12 [label="12"];
    n3  [label="3"];
    n7  [label="7"];
    n14 [label="14"];
    n6  [label="6"];
    n10 [label="10"];

    /* 構造の定義 */
    
    /* 新しい根: 5 */
    n5 -- n2;
    n5 -- n12;

    /* 2 の子 */
    n2 -- n3;

    /* 12 の子 (回転により7が左、元々の14が右) */
    n12 -- n7;
    n12 -- n14;

    /* 7 の子 */
    n7 -- n6;
    n7 -- n10;
}