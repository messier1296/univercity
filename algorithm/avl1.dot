graph AVL_Tree {
    /* ノードの設定: 丸い形にする */
    node [shape=circle];

    /* ノードの定義: IDと表示する数値(label)を分ける */
    n11 [label="11"];
    n5  [label="5"];
    n14 [label="14"];
    n2  [label="2"];
    n8  [label="8"];
    n12 [label="12"];
    n3  [label="3"];
    n7  [label="7"];
    n10 [label="10"];
    n6  [label="6"];

    /* エッジ（線）の定義 */
    
    /* 根 11 の子 */
    n11 -- n5;
    n11 -- n14;

    /* 14 の子 */
    n14 -- n12;

    /* 5 の子 (右の子が7から8に変わっています) */
    n5 -- n2;
    n5 -- n8;

    /* 2 の子 */
    n2 -- n3;

    /* 8 の子 (回転により7と10がここに来ます) */
    n8 -- n7;
    n8 -- n10;

    /* 7 の子 */
    n7 -- n6;
}